<h2> <%= @list.name %> </h2>

<% @list.bookmarks.each do |bookmark| %>
  <li class="">"<%= bookmark.comment %>"</li>
<% end %>

<div class="my-5">
  <%= link_to "Add a movie", new_list_bookmark_path(@list), class: "btn btn-info" %>
  <%= link_to "See all lists", lists_path, class: "btn btn-info" %>
</div>
