<h2>Lists</h2>

<div class="card-list-container">
  <% @lists.each do |list| %>
    <div class="card-list">
      <h5> <%= link_to list.name, list %> </h5>
    </div>
  <% end %>
</div>

<div>
  <%= link_to "Home", root_path, class: "btn btn-info" %>
  <%= link_to "Create a list", new_list_path, class: "btn btn-info" %>
</div>
